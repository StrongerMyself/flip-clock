{
  "extends": ["plugin:@nrwl/nx/react", "./.eslintrc.base.json"],
  "ignorePatterns": ["!**/*"],
  "overrides": [
    {
      "files": ["*.ts", "*.tsx"],
      "rules": {}
    },
    {
      "files": ["*.js", "*.jsx"],
      "rules": {}
    }
  ],
  "rules": {
    "no-unused-expressions": "off",
    "@typescript-eslint/no-unused-expressions": "off"
  }
}
